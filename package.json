{
  "private": true,
  "name": "code-realm",
  "version": "1.0.0",
  "description": "Web development with modern JS",
  "main": "dist/main.js",
  "author": "Alex",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex996/code-realm.git"
  },
  "bugs": {
    "url": "https://github.com/alex996/code-realm/issues"
  },
  "homepage": "https://github.com/alex996/code-realm#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:md": "node --experimental-modules scripts/md2json.mjs",
    "build:ts": "rollup -c",
    "prestart": "rimraf dist",
    "start": "run-p 'build:* -- -w'",
    "serve": "serve dist",
    "commit": "git-cz",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "preact": "^10.0.0-rc.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.0",
    "fecha": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "reading-time": "^1.2.0",
    "rehype-preset-minify": "^3.0.2",
    "rehype-stringify": "^6.0.0",
    "remark-frontmatter": "^1.3.2",
    "remark-parse": "^7.0.0",
    "remark-parse-yaml": "0.0.3",
    "remark-rehype": "^5.0.0",
    "remark-retext": "^3.1.3",
    "retext-english": "^3.0.3",
    "retext-stringify": "^2.0.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-terser": "^5.1.1",
    "serve": "^11.1.0",
    "to-vfile": "^6.0.0",
    "typescript": "^3.5.3",
    "unified": "^8.3.2",
    "vfile-reporter": "^6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12"
  }
}
